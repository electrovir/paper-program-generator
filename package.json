{
    "name": "@electrovir/agenda-editor",
    "version": "1.0.0",
    "description": "Generates printable paper agendas for in-person events.",
    "keywords": [
        "agenda",
        "print",
        "editor",
        "element-vir"
    ],
    "homepage": "https://github.com/electrovir/agenda-editor",
    "bugs": {
        "url": "https://github.com/electrovir/agenda-editor/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/agenda-editor"
    },
    "license": "(MIT or CC0 1.0)",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "virmator frontend build",
        "format": "virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "start": "virmator frontend",
        "test": "exit 0;",
        "test:all": "concurrently --colors --kill-others-on-fail -c auto --names types,tests,spelling,format,deps \"npm run test:types\" \"npm run test:coverage\" \"npm run test:spelling\" \"npm run test:format\" \"npm run test:deps\"",
        "test:coverage": "npm run test coverage",
        "test:deps": "virmator deps check",
        "test:format": "virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/browser": "^26.2.1",
        "@augment-vir/common": "^26.2.1",
        "date-vir": "^5.1.3",
        "element-vir": "^21.0.0",
        "lit-css-vars": "^3.0.9",
        "localforage-esm": "^1.10.2",
        "object-shape-tester": "^2.3.2",
        "prevent-navigation": "^0.0.1",
        "run-time-assertions": "^1.2.0",
        "vira": "^21.0.0"
    },
    "devDependencies": {
        "@augment-vir/node-js": "^26.2.1",
        "cspell": "^8.6.1",
        "dependency-cruiser": "^16.2.4",
        "esbuild": "^0.20.2",
        "npm-check-updates": "~16.12.3",
        "prettier": "^3.2.5",
        "prettier-plugin-interpolated-html-tags": "^1.0.5",
        "prettier-plugin-jsdoc": "^1.3.0",
        "prettier-plugin-multiline-arrays": "^3.0.4",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prettier-plugin-packagejson": "^2.4.14",
        "prettier-plugin-sort-json": "^4.0.0",
        "prettier-plugin-toml": "^2.0.1",
        "type-fest": "^4.14.0",
        "typescript": "5.3.3",
        "virmator": "^11.5.2",
        "vite": "^4.5.2",
        "vite-tsconfig-paths": "^4.3.2"
    },
    "publishConfig": {
        "access": "public"
    }
